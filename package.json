{
  "name": "jarman",
  "version": "1.0.2",
  "url": "https://github.com/thomgray/jarman",
  "description": "jar manager",
  "main": "index.js",
  "scripts": {
    "docker": "docker build docker --tag jarmantestimage",
    "test": "docker run -it -v $PWD:/jarman -w /jarman -e TERM=$TERM jarmantestimage sh /scripts/test.sh",
    "test:debug": "docker run -it -v $PWD:/jarman -w /jarman -e TERM=$TERM jarmantestimage bash",
    "start": "node ./bin/index.js"
  },
  "bin": {
    "jarman": "./bin/index.js"
  },
  "author": "thomddgray@gmail.com",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "mocha": "^7.1.2"
  }
}
